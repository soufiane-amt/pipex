int	child_process(t_data *data, int *fd, char **argv)
	dup2 (fd[1], STDOUT_FILENO);
	close (fd[0]);
	close (fd[1]);
int child_process_t(t_data *data, int *fd, char **argv)
	dup2 (fd[0], STDIN_FILENO);
	close (fd[0]);
	close (fd[1]);
	int		fd[2];
	if (pipe(fd) < 0)
		child_process (&data, fd, argv);
		child_process_t (&data, fd, argv);
